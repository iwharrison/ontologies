:hasLocation rdf:type owl:ObjectProperty ;
	     rdfs:label "has location"@en ;
	     rdfs:comment "an abstract relation between a process or entity and its location"@en ;
	     owl:inverseOf :isLocationOf ;
             rdfs:domain :Process , :Entity ;
             rdfs:range :Location ;
	     rdfs:isDefinedBy syu-ttl: .

:isLocationOf rdf:type owl:ObjectProperty ;
	     rdfs:label "is location of"@en ;
	     rdfs:comment "an abstract relation between a location and a process or entity"@en ;
	     owl:inverseOf :hasLocation ;
             rdfs:domain :Location ;
             rdfs:range :Process , :Entity ;
	     rdfs:isDefinedBy syu-ttl: .

:hasNarrowerTerm a owl:ObjectProperty , owl:TransitiveProperty ;
	rdfs:label "has narrower term"@en ;
	rdfs:comment "A relation between a term and a narrower term. Akin to skos:narrower"@en ;
	rdfs:isDefinedBy syu-ttl: ;
	owl:inverseOf :hasBroaderTerm ;
	rdfs:range owl:Thing ;
	rdfs:domain owl:Thing .

:hasBroaderTerm a owl:ObjectProperty , owl:TransitiveProperty ;
	rdfs:label "has broader term "@en ;
	rdfs:comment "A relation between a term and a broader term. Akin to skos:broader"@en ;
	rdfs:isDefinedBy syu-ttl: ;
